#include <iostream>
#include<bits/stdc++.h>


using namespace std;

int main()
{
    int n[2][3]={
                 {1,43,5},
                 {2,54,1}
                };
    int m[2][3]={
                  {1,3,1},
                  {2,5,6}

                };
   //   sum of matrix n & m
   int sum[2][3];
    for (int i=0;i<2;i++){
            for (int j=0;j<3;j++){
                    sum[i][j]=(n[i][j])+(m[i][j]);
                    cout<<sum[i][j]<<" ";
            }
            printf("\n");
}

    

    return 0;
}
