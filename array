#include <iostream>


using namespace std;

int main()
{
    int n;
    printf("enter the size of an array :");
    scanf("%d",&n);

    int array1[n];

    for (int i=0;i<n;i++){
        scanf("%d",&array1[i]);
    }
    for (int i=0;i<n;i++){
            printf(" the  array elements are as follows : %d \n",array1[i]);
    }



    return 0;
}
