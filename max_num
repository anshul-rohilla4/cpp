#include <iostream>


using namespace std;

int main()
{
    int n;
    printf("enter the size of an array :");
    scanf("%d",&n);

    int array1[n];

    for (int i=0;i<n;i++){
        scanf("%d",&array1[i]);
    }
    int max_num=array1[0];
    for (int i=1;i<n;i++){
            if(array1[i]>max_num){
                max_num=array1[i];

            }
    }
    printf("max element is :%d",max_num);



    return 0;
}
