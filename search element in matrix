#include <iostream>
#include<bits/stdc++.h>


using namespace std;

int main()
{
    int n[3][3]={{2,3,1},{164,5,64},{50,6,8}};
    int flag=0;
    for(int i=0;i<3;i++){
        for (int j=0;j<3;j++){
            if(n[i][j]==0){
                    flag=1;
                cout<<i+1<<" row & "<<j+1<<" column"<<endl;
            }
        }
    }
    if(flag==0){
        cout<<"elemnet does not exist"<<endl;
    }


    return 0;
}
