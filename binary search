#include <iostream>

using namespace std;

int binary(int nums[],int l,int r,int k){
    while(l<=r){

        int mid=l+(r-1)/2;

        if (nums[mid]==k){
                return mid;
        }else if(nums[mid]>k){

        r=mid-1;

    }else{
        l=mid +1;
    }
    }

}

int main()
{
    int nums[]={1,3,4,6,7,9,22,55,99,700,9999};
    int k=1;
    int sixe=sizeof(nums)/sizeof(nums[0]);
    int index=binary(nums,0,sixe-1,k);
    cout<<index;



    return 0;
}
