#include <iostream>


using namespace std;

int main()
{
    int n;
    printf("enter the size of an array :");
    scanf("%d",&n);

    int array1[n];

    for (int i=0;i<n;i++){
        scanf("%d",&array1[i]);
    }
    int sum=0;

    for (int i=0;i<n;i++){
        sum=sum+array1[i];
    }
    printf("sum is  :%d",sum);






    return 0;
}
