#include <iostream>


using namespace std;

int main()
{
    int n;
    printf("enter the size of an array :");
    scanf("%d",&n);

    int array1[n];

    for (int i=0;i<n;i++){
        scanf("%d",&array1[i]);
    }
    int sum=0;

    for (int i=0;i<n;i++){
            if(!(array1[i]%2==0)){
                sum+=1;
            }
    }

            printf("odd num are: %d",sum);
/*
for (int i=0;i<n;i++){
            if(array1[i]%2==0){
                sum+=1;
            }
    }

            printf("even num are: %d",sum);

*/




    return 0;
}
